{
  "name": "universal-tracking",
  "version": "1.0.0",
  "description": "1st-party e-commerce tracking library with modular architecture",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write src/**/*.ts",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "health": "curl http://localhost:3000/health"
  },
  "keywords": [
    "tracking",
    "analytics",
    "ecommerce",
    "1st-party",
    "modular"
  ],
  "author": "Universal Tracking Team",
  "license": "MIT",
  "dependencies": {
    "@fastify/cors": "^11.1.0",
    "clickhouse": "^2.6.0",
    "fastify": "^5.6.1",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.3",
    "pino": "^8.16.2",
    "pino-pretty": "^10.2.3",
    "undici": "^5.28.3",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@jest/globals": "^30.2.0",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.8.10",
    "@types/pg": "^8.10.7",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "eslint": "^8.53.0",
    "eslint-plugin-import": "^2.29.0",
    "jest": "^30.2.0",
    "prettier": "^3.0.3",
    "ts-jest": "^29.4.4",
    "tsx": "^3.14.0",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
